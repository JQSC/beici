{
    "name": "y",
    "displayName": "ese",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:beici-curreny.refresh",
        "onCommand:beici-curreny.complete",
        "onCommand:beici-review.back",
        "onView:beici-curreny",
        "onView:beici-review"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "beici-curreny.refresh",
                "title": "刷新",
                "icon":"resources/dark/refresh.svg"
            },
            {
                "command": "beici-curreny.complete",
                "title": "加入复习列表",
                "icon":"resources/media/ok.svg"
            },
            {
                "command": "beici-review.back",
                "title": "返回学习列表",
                "icon":"resources/dark/back.svg"
            }
        ],
        "views": {
            "beici-explorer": [
                {
                    "id": "beici-curreny",
                    "name": "单词列表"
                },
                {
                    "id": "beici-review",
                    "name": "复习"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "beici-curreny.refresh",
                    "when": "view == beici-curreny",
                    "group": "navigation"
                    
                }
            ],
            "view/item/context": [
                {
                    "command": "beici-curreny.complete",
                    "when": "view == beici-curreny",
                    "group": "inline"
                },
                {
                    "command": "beici-review.back",
                    "when": "view == beici-review",
                    "group": "inline"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "beici-explorer",
                    "title": "Hello English",
                    "icon": "resources/english.svg"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "beici-curreny",
                "contents": "No node dependencies found [learn more](https://www.npmjs.com/).\n[Add Dependency](command:nodeDependencies.addEntry)"
            },
            {
                "view": "beici-review",
                "contents": "No node dependencies found [learn more](https://www.npmjs.com/).\n[Add Dependency](command:nodeDependencies.addEntry)"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }
}